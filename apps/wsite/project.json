{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/wsite",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/wsite",
        "outputPath": "dist/apps/wsite"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/wsite"
        },
        "production": {}
      }
    },
    "postbuild": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "next-sitemap --config apps/wsite/next-sitemap.config.js"
          }
        ]
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "wsite:build",
        "ssl": true,
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "wsite:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "wsite:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "wsite:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/wsite"],
      "options": {
        "jestConfig": "apps/wsite/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/wsite/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
