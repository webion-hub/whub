{
  "name": "whub",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/@wui/*",
    "packages/@wapi/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.7.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "^17.0.45",
    "eslint-config-custom": "workspace:0.0.0",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.18.0",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "5.0.0-alpha.115",
    "@mui/material": "^5.11.3",
    "@mui/system": "^5.11.2",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.0"
  }
}